    "def modify_word(word):\n",
    "    char_list = list(word)\n",
    "    char_list.sort()\n",
    "    return ''.join(char_list)\n",
    "\n",
    "# Exemple d'utilisation\n",
    "original_word = \"abcde\"\n",
    "closest_word = modify_word(original_word)\n",
    "print(closest_word)\n"
   ]
